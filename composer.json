{
    "name": "david/sorted-linked-list-library",
    "description": "Simple library implementing sorted linked list.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SortedLinkedListLibrary\\": "src/"
        }
    },
    "authors": [
        {
            "name": "davidkosvica",
            "email": "david.kosvica@gmail.com"
        }
    ],
    "require": {
      "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "symplify/easy-coding-standard": "^12.0",
        "brianium/paratest": "^7.16"
    },
    "scripts": {
        "test": "paratest --testsuite 'Unit Tests' --processes=auto --functional",
        "test-unit": "paratest --testsuite 'Unit Tests' --processes=auto --functional",
        "test-performance": "paratest --testsuite 'Performance Tests' --processes=auto --functional",
        "test-all": "paratest --testsuite 'Unit Tests' --processes=auto --functional && paratest --testsuite 'Performance Tests' --processes=auto --functional",
        "analyse": "php -d memory_limit=512M vendor/bin/phpstan analyse",
        "ecs": "ecs check",
        "ecs-fix": "ecs check --fix"
    }
}
